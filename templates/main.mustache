<section>
    <div class="card-body">
        <h3 class="lead">{{#str}}naplan_results_report, block_naplan_results_report {{/str}}</h3>
        <span id="scale-info" class="fa fa-info-circle "></span>
        <div class="container-fluid">
            <div class="row align-items-center">
                <div class="col-lg-12">
                    <dl>
                        <dd>
                            <ul class="badges">
                                {{#hasdata}}
                                {{> block_naplan_results_report/naplan_result}}
                                {{/hasdata}}

                                {{^hasdata}}
                                {{> block_naplan_results_report/naplan_no_data}}
                                {{/hasdata}}
                            </ul>
                        </dd>
                    </dl>
                </div>
            </div>
        </div>
    </div>
</section>
